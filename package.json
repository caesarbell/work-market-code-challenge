{
  "name": "questionnaire",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm precommit",
      "pre-push": "npm precommit"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged && npm test",
    "start": "PORT=8080 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "husky": "^1.3.1",
    "info": "^1.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  }
}
